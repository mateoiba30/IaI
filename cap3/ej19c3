programa ej19c3
areas
  ciudad:AreaC(1,1,100,100)
robots
  robot robot1
  variables
    papeles:numero
  comenzar
    repetir 4
      repetir 99
        mientras(HayPapelEnLaEsquina)
          tomarPapel
          papeles:=papeles+1
        mover
      derecha
    Informar(papeles)

  fin
variables
  Rinfo:robot1
comenzar
  AsignarArea(Rinfo,ciudad)
  Iniciar(Rinfo,1,1)
fin
