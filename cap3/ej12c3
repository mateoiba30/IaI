programa ej12c3
areas
  ciudad:AreaC(1,1,100,100)
robots
  robot robot1
  variables
    flores:numero
  comenzar
    Pos(1,7)
    mientras ((flores<20)&(PosCa<100))
      si(HayFlorEnLaEsquina)
        tomarFlor
        flores:=flores+1
      si (flores<20)
        mover
  fin
variables
  Rinfo:robot1
comenzar
  AsignarArea(Rinfo,ciudad)
  Iniciar(Rinfo,1,1)
fin
